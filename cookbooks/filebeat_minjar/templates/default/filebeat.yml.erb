filebeat:
  prospectors:
    -
      paths:
        - <%= @log_path %>
      encoding: plain
      input_type: log
      fields:
        log_type: access
  registry_file: /var/lib/filebeat/registry
output:
  logstash:
    hosts: ["<%= @logstash_hosts %>:<%= @logstash_port %>"]
shipper:
logging:
  to_syslog: false
  to_files: true
files:
    path: /var/log/filebeat
    name: filebeat.log
    rotateeverybytes: 10485760  # = 10MB
    level: info